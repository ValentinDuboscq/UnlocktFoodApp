{
  "expo": {
    "name": "react-native-app-tech-test",
    "slug": "react-native-app-tech-test",
    "version": "1.0.0",
    "scheme": "unlockt-app",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.reactnativeapptechtest",
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app requires access to your health data to track your daily calorie expenditure.",
        "NSHealthUpdateUsageDescription": "This app requires access to your health data to record your workouts."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.reactnativeapptechtest"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "react-native-health"]
  }
}
